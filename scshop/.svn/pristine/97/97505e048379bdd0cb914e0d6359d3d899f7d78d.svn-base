			}
			.ms-sp-tit{
				width:100%;
				line-height:.6rem;
				font-weight:600;
				@include overflow-1row();
			}
			.ms-price{
				width:100%;
				color:$ele-fontcolor;
				@include overflow-1row();

				strong{
					font-size:$ele-size16;
				}
				span{
					font-size:$fontSize;
					color:#666;
					margin-left:.2rem;
				}
				.ms-zk{
					display:inline-block;
					padding:.03rem .08rem;
					background-color:$ele-backcolor;
					color:#fff;
					font-size:$fontSize;
					position:relative;
					top:-.04rem;
					@include radius(.04rem);
				}
			}
			.ms-djs{
				width:100%;
				line-height:.5rem;

				span{
					color:$ele-fontcolor;
				}
			}
		}
	}
}
.emptybox{
	width:100%;
	text-align:center;
	padding:2.2rem 0;
	color:#666;

	i{
		font-size:1.5rem;
	}
	span{
		display:block;
		margin-top:.3rem;
		font-size:$ele-size17;
	}
	a{
		display:inline-block;
		padding:0 .4rem;
		line-height:.64rem;
		font-size:$ele-size15;
		margin-top:.35rem;
		background-color:$ele-backcolor;
		color:#fff;
		@include radius(.04rem);
	}
}
/* miaosha style end */


/* tixian style start */
.tx-box{
    width:100%;
}
.tx-total{
	width:100%;
	line-height:1rem;
	padding:0 $ele-padding;
	background-color:#fff;
	font-size:$ele-size13;
	position:relative;

	&:before{
		@include lineafter-top();
	}
	&:after{
		@include lineafter-bottom();
	}
	span{
		color:$ele-fontcolor;
			font-size:$ele-size14;
	}
	a{
		float:right;
		display:inline-block;
	}
}
.tx-list{
	width:100%;
	height:1rem;
	line-height:1rem;
	padding:0 $ele-padding;
	font-size:$ele-size13;
	background-color:#fff;
	position:relative;

	&:before{
		@include lineafter-top();
	}
	&:after{
		@include lineafter-bottom();
	}
	.txtit{
		width:1.3rem;
		display:block;
		position:absolute;
		top:0;
		left:.2rem;
	}
	.tx-cont{
		width:100%;
		padding-left:1.3rem;
		position:relative;

		&:after{
			@include clearfix();
		}
		.txfs-lab{
			display:block;
			width:50%;
			float:left;
			position:relative;
			-webkit-tap-highlight-color: rgba(0,0,0,0);

			.txfsipt{
				position:absolute;
				top:0;
				left:0;
				opacity:0;
				width:100%;
				height:100%;
				font-size:300%;
				outline:none;
				-webkit-appearance:none;
			}
			i{
				margin-right:.1rem;
				font-size:$ele-size15;
				color:#999;
			}
			.txfsipt:checked+i{
				color:$ele-fontcolor;
			}
		}
	}
}
.tx-marg{
	margin-top:$ele-margin;
}
.tx-group{
	width:100%;
	font-size:$ele-size13;
	background-color:#fff;
	padding:0 $ele-padding;
	margin-top:$ele-margin;
	position:relative;
	&:before{
		@include lineafter-top();
	}
	&:after{
		@include lineafter-bottom();
	}
	.tx-g-list{
		width:100%;
		height:1rem;
		line-height:1rem;
		position:relative;

		&:after{
    		@include lineafter-bottom();
    	}
    	&:last-child:after{
			height:0;
		}
		.txtit{
    		width:1.3rem;
    		display:block;
    		position:absolute;
    		text-align:right;
    		top:0;
    		left:0;
    	}
    	.tx-container{
    		width:100%;
    		padding-left:1.4rem;
    		position:relative;
    	}
    	input{
    		width:100%;
    		height:100%;
    		height:1rem;
    		border:0;
    		outline:none;
    		padding:0;
    		margin:0;
    		font-size:$ele-size13;
			-webkit-appearance:none;
			-webkit-tap-highlight-color: rgba(0,0,0,0);
    	}
    	.yhkxz{
    		display:block;
    		position:relative;
    		-webkit-tap-highlight-color: rgba(0,0,0,0);

    		i{
    			float:right;
    			color:#999;
    			font-size:$ele-size12;
    		}
    	}
	}    	
}
#bank{
	display:none;
}
.tx-btn{
	width:93%;
	height:.84rem;
	margin:.6rem auto;
	border:0;
	display:block;
	font-size:$ele-size14;
	color:#fff;
	outline:none;
	-webkit-appearance:none;
	background-color:$ele-backcolor;
	@include radius(.05rem);
}

.modal{
	position:fixed;
	top:30%;
	left:5%;
	z-index:1001;
	width:90%;
	display:none;
	background-color:#fff;
	@include radius(.05rem);

	.modal-text{
		padding:.4rem .3rem;
		font-size:$ele-size14;
		line-height:.4rem;
	}
	.modal-ipt{
		width:100%;
		padding:0 .3rem;
		padding-bottom:.4rem;
		font-size:$ele-size13;
		position:relative;

		&:after{
			@include clearfix();
		}
		.yzmtext{
			width:60%;
			float:left;
			height:.7rem;
			outline:none;
			-webkit-appearance:none;
			border:1px solid #ddd;
			@include radius(.05rem);
		}
		.yzmbtn{
			width:38%;
			height:.7rem;
			outline:none;
			-webkit-appearance:none;
			border:0;
			background-color:#ddd;
			float:right;
			@include radius(.05rem);
		}
	}
	.modal-btns{
		width:100%;
		position:relative;
		@include flexbox();
		&:before{
			@include lineafter-top();
		}

		a{
			width:100%;
			line-height:.84rem;
			font-size:$ele-size13;
			text-align:center;
			-webkit-tap-highlight-color: rgba(0,0,0,0);
			position:relative;

			@include boxflex();
			&:after{
				@include lineafter-right();
			}
			&:last-child:after{
				width:0;
			}
			&.confirmbtn{
				background-color:$ele-backcolor;
				border-bottom-left-radius:.05rem;
				color:#fff;
			}
		}
	}
}

.modal-overlay{
    position:fixed;
    left:0;
    top:0;
    width:100%;
    height:100%;
    background:rgba(0, 0, 0, 0.4);
    z-index:1000;
    visibility:hidden;
    opacity:0;
    -webkit-transition-duration:400ms;
    transition-duration:400ms;

    &.modal-overlay-visible{
		visibility:visible;
	    opacity:1;
	}
}

/* tixian style end */


