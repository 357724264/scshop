@import "variables","mixins","compass/reset";

*{
    box-sizing:border-box;
}
body{
    font-size: $fontSize;
    color: $bodycolor;
    background-color:$bodyback;
    font-family:$mainline-ff;
}
a{
    color:$bodycolor;
    text-decoration:none;
    -webkit-tap-highlight-color: rgba(0,0,0,0);
}
.content{
	width:100%;
	position:relative;
}


/* mycoupon style start */
$shopshead-height:.9rem;
$fltabs-height:.74rem;
$couponnav-height:1rem;
$ad-newbtn:1.1rem;
$add-bank:.9rem;

.paddbody{
	padding-top:$shopshead-height + $fltabs-height;
	padding-bottom:$couponnav-height + .2rem;
}
.nav-padd{
	padding-top:$shopshead-height;
	padding-bottom:$couponnav-height + .2rem;
}
.address-padd{
	padding-top:$shopshead-height;
	padding-bottom:$ad-newbtn + .2rem;
}
.yhjlq-padd{
	padding-top:$shopshead-height;
}
.add-bankpadd{
	padding-top:$shopshead-height;
	padding-bottom:$add-bank;
}

.shops-head{
	width:100%;
	height:$shopshead-height;
	position:fixed;
	top:0;
	left:0;
	z-index:100;
	text-align:center;
	background-color:#fff;

	&:after{
		@include lineafter-bottom();
	}

	.head-left{
		position:absolute;
		top:0;
		left:0;
		z-index:2;
		width:.8rem;
		height:$shopshead-height;
		line-height:$shopshead-height;

		i{
			font-size:.4rem;
		}
	}
	.head-tit{
		width:100%;
		height:100%;
		padding:.11rem;
		line-height:.68rem;
		padding-left:$shopshead-height;
		padding-right:$shopshead-height;
		position:relative;
		font-size:$ele-size17;
		font-weight:600;

		@include overflow-1row();
	}
	.head-right{
		position:absolute;
		top:0;
		right:0;
		z-index:2;
		width:.8rem;
		height:$shopshead-height;
		line-height:$shopshead-height;

		i{
			font-size:.45rem;
		}
	}
}

.fltabs{
	width:100%;
	height:.74rem;
	line-height:.74rem;
	text-align:center;
	background-color:#fff;
	position:fixed;
	top:$shopshead-height;
	left:0;
	z-index:9;
	@include flexbox();

	&:after{
		@include lineafter-bottom();
	}

	.tab-link{
		@include boxflex();
		display:block;
		width:100%;
		text-align:center;
		position:relative;
		font-size:$ele-size13;
		
		&.active:after{
			@include lineafter-bottom2(3px);
		}
	}
}
.tab{
	display:none;
}
.tab.active{
	display:block;
}

.coupon-box{
	width:100%;
	padding:0 $ele-padding;

	.coupon-item{
		width:100%;
		background-color:#fff;
		border:.01rem solid #ddd;
		border-top-left-radius:.1rem;
		border-top-right-radius:.1rem;
		margin-top:.2rem;

		.cp-cont{
			width:100%;
			border-top:.15rem solid #57b6e2;
			border-bottom:.02rem dashed #ddd;
			border-top-left-radius:.1rem;
			border-top-right-radius:.1rem;
			padding:.3rem .1rem;
			position:relative;

			.cp-jz{
				display:inline-block;
				color:#57b6e2;
				font-size:$ele-size13;
				line-height:.8rem;

				strong{
					font-size:.5rem;
				}
			}
			.cp-dpm{
				position:absolute;
				top:.3rem;
				right:.1rem;
				display:inline-block;
				max-width:3.8rem;
				line-height:.4rem;
				font-size:$ele-size13;
				text-align:center;
				.cp-dp-name{
					border-bottom:.01rem solid #ddd;
				}
			}
			.cp-othsm{
				font-size:$ele-size12;
				line-height:.26rem;
				margin-top:.2rem;
			}
		}
		.cp-time{
			line-height:.6rem;
			padding:0 .1rem;
			font-size:$ele-size12;
			color:#666;
		}
	}
	&.overdue{
		filter:progid:DXImageTransform.Microsoft.BasicImage(grayscale=1);
    	-webkit-filter:grayscale(100%);
	}
}

.coupon-nav{
	position:fixed;
	bottom:0;
	left:0;
	z-index:100;
	background-color:#fff;
	width:100%;
	height:$couponnav-height;
	line-height:.74rem;
	padding:.12rem .2rem;
	text-align:center;

	&:before{
		@include lineafter-top();
	}

	a{
		border:.01rem solid transparent;
		border-color:$ele-fontcolor;
		color:$ele-fontcolor;
		font-size:$ele-size13;
		@include radius(.05rem);
		display:block;
	}
}

.yhjlqbtn{
	display:inline-block;
	padding:0 .5rem;
	font-size:$ele-size13;
	line-height:.6rem;
	border:.01rem solid #ddd;
	-webkit-tap-highlight-color: rgba(0,0,0,0);
}

/* mycoupon style end */


/* miaosha style start */
.ms-padd{
	padding-top:$shopshead-height + $fltabs-height;
}
.sdg-padd{
	padding-top:$shopshead-height;
}

$a-padditb:.36rem;
$a-paddilr:.1rem;
$msimg-width:1.7rem;
$msimg-height:1.7rem;

.msbox{
	width:100%;
	padding:$ele-padding;

	dd{
		width:100%;
		background-color:#fff;
		border:.01rem solid #ddd;
		margin-bottom:$ele-margin;
		position:relative;

		a{
			display:block;
			padding:$a-padditb $a-paddilr;
			position:relative;
		}
		.ms-img{
			position:absolute;
			top:$a-padditb;
			left:$a-paddilr;
			width:$msimg-width;
			height:$msimg-height;

			img{
				width:100%;
				height:100%;
			}
		}
		.ms-cont{
			width:100%;
			min-height:$msimg-height;
			padding-left:$msimg-width + .15rem;
			font-size:$ele-size13;

			.ms-gj{
				width:100%;
				display:table-cell;
				vertical-align:middle;
				font-size:$ele-size13;

				img{
					width:.3rem;
				    height:.3rem;
				    display:inline-block;
				    vertical-align:middle;
				}
