		padding-left:1.6rem;
		position:relative;
		line-height:.9rem;

		&:after{
			@include lineafter-bottom();
		}
		label{
			position:absolute;
			top:0;
			width:1.2rem;
			left:$ele-padding;
			text-align:right;
		}
		input{
			width:100%;
			height:.9rem;
			border:0;
			outline:none;
			font-size:$ele-size13;
			-webkit-appearance:none;
			-webkit-tap-highlight-color: rgba(0,0,0,0);
		}
	}
}
.mrad{
	width:100%;
	line-height:.9rem;
	font-size:$ele-size14;
	position:relative;
	margin-top:$ele-margin;
	background-color:#fff;
	padding:0 $ele-padding;

	&:before{
		@include lineafter-top();
	}
	&:after{
		@include lineafter-bottom();
	}
	i{
		font-size:$ele-size17;
		color:#999;
	}
	input[type=checkbox]{
		width:100%;
		height:100%;
		position:absolute;
		top:0;
		left:0;
		z-index:2;
		opacity:0;
		outline:none;
		-webkit-appearance:none;

		&:checked+i{
			color:$ele-fontcolor;
		}
	}
}
/*add-address style end*/

/*kefu style start*/
$kf-textbox:.9rem;
.kf-padd{
	padding-top:$shopshead-height;
	padding-bottom:$kf-textbox;
}
#wrapper{
	position: absolute;
    z-index: 1;
    top:0;
    bottom:0;
    left: 0;
    width: 100%;
    overflow: hidden;
}
#scroller{position:absolute;}
#scroller-pullDown{
    height:.8rem;
    line-height:.8rem;
    padding:.05rem .1rem;		    
    text-align: center;
    position: absolute;
    top:-.9rem;
    left: 0px;
    width: 100%;
}
.preloader {
  	display: inline-block;
  	width: .4rem;
  	height: .4rem;
  	background-image: url("data:image/svg+xml;charset=utf-8,%3Csvg%20viewBox%3D'0%200%20120%20120'%20xmlns%3D'http%3A%2F%2Fwww.w3.org%2F2000%2Fsvg'%20xmlns%3Axlink%3D'http%3A%2F%2Fwww.w3.org%2F1999%2Fxlink'%3E%3Cdefs%3E%3Cline%20id%3D'l'%20x1%3D'60'%20x2%3D'60'%20y1%3D'7'%20y2%3D'27'%20stroke%3D'%236c6c6c'%20stroke-width%3D'11'%20stroke-linecap%3D'round'%2F%3E%3C%2Fdefs%3E%3Cg%3E%3Cuse%20xlink%3Ahref%3D'%23l'%20opacity%3D'.27'%2F%3E%3Cuse%20xlink%3Ahref%3D'%23l'%20opacity%3D'.27'%20transform%3D'rotate(30%2060%2C60)'%2F%3E%3Cuse%20xlink%3Ahref%3D'%23l'%20opacity%3D'.27'%20transform%3D'rotate(60%2060%2C60)'%2F%3E%3Cuse%20xlink%3Ahref%3D'%23l'%20opacity%3D'.27'%20transform%3D'rotate(90%2060%2C60)'%2F%3E%3Cuse%20xlink%3Ahref%3D'%23l'%20opacity%3D'.27'%20transform%3D'rotate(120%2060%2C60)'%2F%3E%3Cuse%20xlink%3Ahref%3D'%23l'%20opacity%3D'.27'%20transform%3D'rotate(150%2060%2C60)'%2F%3E%3Cuse%20xlink%3Ahref%3D'%23l'%20opacity%3D'.37'%20transform%3D'rotate(180%2060%2C60)'%2F%3E%3Cuse%20xlink%3Ahref%3D'%23l'%20opacity%3D'.46'%20transform%3D'rotate(210%2060%2C60)'%2F%3E%3Cuse%20xlink%3Ahref%3D'%23l'%20opacity%3D'.56'%20transform%3D'rotate(240%2060%2C60)'%2F%3E%3Cuse%20xlink%3Ahref%3D'%23l'%20opacity%3D'.66'%20transform%3D'rotate(270%2060%2C60)'%2F%3E%3Cuse%20xlink%3Ahref%3D'%23l'%20opacity%3D'.75'%20transform%3D'rotate(300%2060%2C60)'%2F%3E%3Cuse%20xlink%3Ahref%3D'%23l'%20opacity%3D'.85'%20transform%3D'rotate(330%2060%2C60)'%2F%3E%3C%2Fg%3E%3C%2Fsvg%3E");
  	background-position: 50%;
  	background-size: 100%;
  	background-repeat: no-repeat;
  	-webkit-animation: preloader-spin 1s steps(12, end) infinite;
  	animation: preloader-spin 1s steps(12, end) infinite;
}
@-webkit-keyframes preloader-spin {
  	100% {
    	-webkit-transform: rotate(360deg);
  	}
}
@keyframes preloader-spin {
  	100% {
    	transform: rotate(360deg);
  	}
}

$tjcpimg:1.75rem;
.tjcp-box{
	width:100%;
	margin-top:.2rem;
	padding-left:$tjcpimg;
	background-color:#fff;
	box-shadow:0 0 1px #ddd,0 0 1px #ddd;
	-webkit-box-shadow:0 0 1px #ddd,0 0 1px #ddd;
	-moz-box-shadow:0 0 1px #ddd,0 0 1px #ddd;
	-ms-box-shadow:0 0 1px #ddd,0 0 1px #ddd;
	-o-box-shadow:0 0 1px #ddd,0 0 1px #ddd;
	position:relative;
	@include radius(.05rem);

	.tjcp-img{
		width:$tjcpimg;
		height:1.5rem;
		padding:$ele-padding 0;
		text-align:center;
		position:absolute;
		top:0;
		left:0;

		img{
			height:100%;
			width:auto;
			max-width:100%;
		}
	}
	.tjcp-cont{
		width:100%;
		font-size:$ele-size13;
		min-height:1.5rem;
		padding:$ele-padding .15rem;

		.tjcp-tit{
			width:100%;
			font-size:$ele-size15;
			line-height:.38rem;

			@include overflow-1row();
		}
		.tjcp-price{
			width:100%;
			line-height:.38rem;
		}
		.tjcp-link{
			display:inline-block;
			padding:0 .2rem;
			line-height:.38rem;
			color:#fff;
			background-color:$ele-backcolor;
			@include radius(.05rem);
		}
	}
}
.kefu-box{
	width:100%;
	padding:0 $ele-padding;
	padding-bottom:.2rem;
	position:relative;

	.kf-lrbox{
		width:100%;
		min-height:.8rem;
		padding:0 1.2rem;
		margin-top:$ele-margin;
		position:relative;

		.kf-avatar{
			width:1rem;
			height:.8rem;
			padding:0 .1rem;
			position:absolute;
			top:0;
			left:0;

			img{
				width:100%;
				height:100%;
				@include radius50();
			}
		}
		.kf-lr{
			display:inline-block;
			background-color:#fff;
			padding:.24rem;
			font-size:$ele-size13;
			line-height:.32rem;
			box-shadow:0 0 1px #ddd,0 0 1px #ddd;
			-webkit-box-shadow:0 0 1px #ddd,0 0 1px #ddd;
			-moz-box-shadow:0 0 1px #ddd,0 0 1px #ddd;
			-ms-box-shadow:0 0 1px #ddd,0 0 1px #ddd;
			-o-box-shadow:0 0 1px #ddd,0 0 1px #ddd;
			position:relative;

			@include radius(.05rem);

			.sj-l{
				position:absolute;
				width:0;
				height:0;
				z-index:2;
				display:block;
				top:.27rem;
				left:-.23rem;
				border-top:.1rem solid transparent;
				border-bottom:.2rem solid transparent;
				border-right:.27rem solid #fff;
			}
		}
	}
	.kh-lrbox{
		width:100%;
		min-height:.8rem;
		padding:0 1.2rem;
		margin-top:$ele-margin;
		position:relative;

		&:after{
			@include clearfix();
		}
		.kh-avatar{
			width:1rem;
			height:.8rem;
			padding:0 .1rem;
			position:absolute;
			top:0;
			right:0;

			img{
				width:100%;
				height:100%;
				@include radius50();
			}
		}
		.kh-lr{
			float:right;
			display:inline-block;
			background-color:#7fcaf1;
			padding:.24rem;
			font-size:$ele-size13;
			line-height:.32rem;
			box-shadow:0 0 1px #ddd,0 0 1px #ddd;
			-webkit-box-shadow:0 0 1px #ddd,0 0 1px #ddd;
			-moz-box-shadow:0 0 1px #ddd,0 0 1px #ddd;
			-ms-box-shadow:0 0 1px #ddd,0 0 1px #ddd;
			-o-box-shadow:0 0 1px #ddd,0 0 1px #ddd;
			position:relative;

			@include radius(.05rem);

			.sj-r{
				position:absolute;
				width:0;
				height:0;
				z-index:2;
				display:block;
				top:.27rem;
				right:-.23rem;
				border-top:.1rem solid transparent;
				border-bottom:.2rem solid transparent;
				border-left:.27rem solid #7fcaf1;
			}
		}
	}
	.time{
		text-align:center;
		margin-top:.4rem;
		margin-bottom:.1rem;
		color:#999;
	}
}

.kf-textbox{
	width:100%;
	height:$kf-textbox;
	padding:.1rem $ele-padding;
	padding-right:1.3rem;
	background-color:#e6e6e6;
	position:fixed;
	bottom:0;
	left:0;
	z-index:800;

	&:before{
		@include lineafter-top();
	}
	.kf-text{
		width:100%;
		height:100%;
		padding:.02rem;
		outline:none;
		border:0;
		resize:none;
		font-size:$ele-size13;
		-webkit-appearance:none;

		@include radius(.1rem);
	}
	.kf-btn{
		width:1.3rem;
		height:$kf-textbox;
		line-height:$kf-textbox;
		text-align:center;
		font-size:$ele-size15;
		position:absolute;
		right:0;
		top:0;
		z-index:2;
		outline:none;
		-webkit-appearance:none;
		border:0;
		color:#666;
	}
}


/*kefu style end*/