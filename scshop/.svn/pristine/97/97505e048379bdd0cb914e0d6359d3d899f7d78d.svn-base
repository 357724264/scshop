/* applyfor style start */
.af-box{
	width:100%;
	background-color:#fff;
	padding:0 $ele-padding;
}
.af-tit{
	width:100%;
	line-height:.7rem;
	padding-top:.15rem;
	font-size:$ele-size15;
	.bx{
		color:$ele-fontcolor;
	}
	.mark{
		color:#999;
		font-size:$ele-size13;
	}
}
.af-type{
	width:100%;
	position:relative;
	&:before{
		content:'';
	    position:absolute;
	    left:-0.5px;
	    bottom:0;
	    right:auto;
	    top:auto;
	    height:100%;
	    width:1px;
	    background-color:#000;
	    display:block;
	    z-index:2;
	    -webkit-transform-origin:100% 50%;
	    transform-origin:100% 50%;
	    -webkit-transform: scaleX(.5);
	    transform: scaleX(.5);
	    backface-visibility:hidden;
	}
	&:after{
		content:'';
	    position:absolute;
	    left:auto;
	    bottom:auto;
	    right:0;
	    top:0;
	    height:100%;
	    width:1px;
	    background-color:#000;
	    display:block;
	    z-index:2;
	    -webkit-transform-origin:100% 50%;
	    transform-origin:100% 50%;
	    -webkit-transform: scaleX(.5);
	    transform: scaleX(.5);
	    backface-visibility:hidden;
	}

	li{
		position:relative;
		line-height:.8rem;
		padding:0 .15rem;
		font-size:$ele-size13;
	
		&:before{
			content:'';
		    position:absolute;
		    left:0;
		    bottom:auto;
		    right:auto;
		    top:-1px;
		    height:1px;
		    width:100%;
		    background-color:#000;
		    display:block;
		    z-index:2;
		    -webkit-transform-origin:50% 100%;
		    transform-origin:50% 100%;
		    -webkit-transform: scaleY(.5);
		    transform: scaleY(.5);
		    backface-visibility:hidden;
		}
		&:after{
			content:'';
		    position:absolute;
		    left:0;
		    bottom:0;
		    right:auto;
		    top:auto;
		    height:1px;
		    width:100%;
		    background-color:#000;
		    display:block;
		    z-index:2;
		    -webkit-transform-origin:50% 100%;
		    transform-origin:50% 100%;
		    -webkit-transform: scaleY(.5);
		    transform: scaleY(.5);
		    backface-visibility:hidden;
		}
		input[type=radio]{
			width:100%;
			height:100%;
			position:absolute;
			top:0;
			left:0;
			-webkit-appearance:none;
			outline:none;
			opacity:0;
			resize:none;
			border:0;
			z-index:2;
			-webkit-tap-highlight-color: rgba(0,0,0,0);

