function xingji(id){
	$(id).each(function(index){
        var html='<img src="/static/images/pj1.png" /><img src="/static/images/pj1.png" /><img src="/static/images/pj1.png" /><img src="/static/images/pj1.png" /><img src="/static/images/pj1.png" />';
        $(this).html(html);

        var thisval=parseInt($(this).attr('data-val'))/100;
        if(thisval<0.2){
            var html='<img src="/static/images/pj3.png" /><img src="/static/images/pj1.png" /><img src="/static/images/pj1.png" /><img src="/static/images/pj1.png" /><img src="/static/images/pj1.png" />';
            $(this).html(html);
        }
        else if(thisval==0.2){
            var html='<img src="/static/images/pj2.png" /><img src="/static/images/pj1.png" /><img src="/static/images/pj1.png" /><img src="/static/images/pj1.png" /><img src="/static/images/pj1.png" />';
            $(this).html(html);
        }
        else if(thisval<0.4){
            var html='<img src="/static/images/pj2.png" /><img src="/static/images/pj3.png" /><img src="/static/images/pj1.png" /><img src="/static/images/pj1.png" /><img src="/static/images/pj1.png" />';
            $(this).html(html);
        }
        else if(thisval==0.4){
            var html='<img src="/static/images/pj2.png" /><img src="/static/images/pj2.png" /><img src="/static/images/pj1.png" /><img src="/static/images/pj1.png" /><img src="/static/images/pj1.png" />';
            $(this).html(html);
        }
        else if(thisval<0.6){
            var html='<img src="/static/images/pj2.png" /><img src="/static/images/pj2.png" /><img src="/static/images/pj3.png" /><img src="/static/images/pj1.png" /><img src="/static/images/pj1.png" />';
            $(this).html(html);
        }
        else if(thisval==0.6){
            var html='<img src="/static/images/pj2.png" /><img src="/static/images/pj2.png" /><img src="/static/images/pj2.png" /><img src="/static/images/pj1.png" /><img src="/static/images/pj1.png" />';
            $(this).html(html);
        }
        else if(thisval<0.8){
            var html='<img src="/static/images/pj2.png" /><img src="/static/images/pj2.png" /><img src="/static/images/pj2.png" /><img src="/static/images/pj3.png" /><img src="/static/images/pj1.png" />';
            $(this).html(html);
        }
        else if(thisval==0.8){
            var html='<img src="/static/images/pj2.png" /><img src="/static/images/pj2.png" /><img src="/static/images/pj2.png" /><img src="/static/images/pj2.png" /><img src="/static/images/pj1.png" />';
            $(this).html(html);
        }
        else if(thisval<1){
            var html='<img src="/static/images/pj2.png" /><img src="/static/images/pj2.png" /><img src="/static/images/pj2.png" /><img src="/static/images/pj2.png" /><img src="/static/images/pj3.png" />';
            $(this).html(html);
        }
        else if(thisval>=1){
            var html='<img src="/static/images/pj2.png" /><img src="/static/images/pj2.png" /><img src="/static/images/pj2.png" /><img src="/static/images/pj2.png" /><img src="/static/images/pj2.png" />';
            $(this).html(html);
        }
        
    })
}