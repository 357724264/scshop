/* '娑擃亙姹夋穱鈩冧紖' style start */
.grxx-box{
	width:100%;
	padding:0 .5rem;
	background-color:#fff;
	position:relative;
}
.grxx-box table{
	width:100%;
}
.grxx-box table td{
	padding:.5rem 0;
	font-size:.65rem;
	line-height:1.2rem;
	position:relative;
}
.grxx-box table td:after{
	content:'';
    position:absolute;
    left:0;
    bottom:0;
    right:auto;
    top:auto;
    height:1px;
    width:100%;
    background-color:#ddd;
    display:block;
    z-index:1;
    -webkit-transform-origin:50% 100%;
    transform-origin:50% 100%;
    -webkit-transform: scaleY(.5);
    transform: scaleY(.5);
    backface-visibility:hidden;
}
.grxx-box table tr:last-child td:after{
	height:0;
}
.grxx-box table td.td1{
	width:4rem;
}
.grxx-box table td.td2{
	text-align:right;
	color:#666;
}
.grxx-box table .abm-img{
	width:100%;
	height:3rem;
	position:relative;
}
.grxx-box table .abm-img input{
	position:absolute;
	top:0;
	right:0;
	width:3rem;
	height:3rem;
	opacity:0;
	z-index:2;
}
.grxx-box table .abm-img img{
	width:3rem;
	height:100%;
	position:relative;
	z-index:1;
}
#upimg{
	width:3rem;
	height:100%;
	position:absolute;
	top:0;
	right:0;
	opacity:0;
	font-size:300%;
	z-index:2;
}
.obm-tit{
	width:100%;
	margin:0;
	line-height:2rem;
	padding:0 .5rem;
	font-size:.75rem;
}
.grxx-box table td a{
	display:block;
	color:#666;
}
