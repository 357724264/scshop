/* '閸熷棗鎼х拠鍕幆' style start */
.shppbox{
	width:100%;
	padding:0 .5rem;
	margin-bottom:.5rem;
	background-color:#fff;
	position:relative;
}
.shppbox .shppimage{
	display:block;
