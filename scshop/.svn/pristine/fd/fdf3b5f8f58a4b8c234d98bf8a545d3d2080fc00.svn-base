/*utf-8*/
/*global style start*/
body{
	margin:0 auto;
}
.padd-content{
	padding-bottom:.5rem;
}

ul,li{
	margin:0;
	padding:0;
	list-style:none;
}
dl,dd{
	margin:0;
	padding:0;
}
.bar-tab .tab-item.active, .bar-tab .tab-item:active{
	color:#d71416;
}
.bar .icon{
	font-size:1.2rem;
	line-height:2.2rem;
}

#second-bar.bar .button-link{
	color:#444;
}
.clicked{
	filter: progid:DXImageTransform.Microsoft.BasicImage(grayscale=1);
  	-webkit-filter: grayscale(100%);
  	-moz-filter: grayscale(100%);
  	-ms-filter: grayscale(100%);
  	-o-filter: grayscale(100%);
  	filter:grayscale(100%);
  	background-color:#ddd;
  	color:#999;
  	pointer-events: none;
}
.clicked a{
	background-color:#3e3e3e !important;
}
.after:after{
	content:'';
    position:absolute;
    left:0;
    bottom:0;
    right:auto;
    top:auto;
    height:1px;
    width:100%;
    background-color:#ddd;
    display:block;
    z-index:2;
    -webkit-transform-origin:50% 100%;
    transform-origin:50% 100%;
    -webkit-transform: scaleY(.5);
    transform: scaleY(.5);
    backface-visibility:hidden;
}
/*global style end*/

/* '妫ｆ牠銆� style start */
.scshop-index{
	background-color:#d71416;
}
.scshop-index.bar .button-link{
	color:#fff;
}
#scshop-logo{
	padding:.3rem 0;
}
.scshop-index .search-index{
	position:absolute;
	top:0;
	left:0;
	width:100%;
	height:2.2rem;
	padding:.3rem 48px 0.3rem 75px;
	box-sizing:border-box;
	overflow:hidden;
}
.scshop-index .sousuo-box{
	width:100%;
	height:100%;
	position:relative;
}
.scshop-index .sousuo-icon{
	position:absolute;
	top:0;
	left:.3rem;
	width:1.2rem;
	line-height:1.6rem;
	font-size:1rem;
	color:#fff;
	z-index:1;
}
.scshop-index .search{
	margin:0 !important;
	padding:0;
	border:0;
	width:100%;
	height:100% !important;
	border-radius:.12rem;
	position:relative;
	background-color:#c01112;
	z-index:-1;
	outline:none;
	-webkit-appearance:none;
}

.classification{
	width:100%;
	height:2.2rem;
	background-color:#fff;

	-webkit-align-self: center;
    align-self: center;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-lines: single;
    -moz-box-lines: single;
    -webkit-flex-wrap: nowrap;
    flex-wrap: nowrap;
}
.button{
    color: #0894ec;
    text-decoration: none;
    text-align: center;
    display: block;
    border-radius: .25rem;
    line-height: 1.25rem;
    box-sizing: border-box;
    -webkit-appearance: none;
    -moz-appearance: none;
    -ms-appearance: none;
    appearance: none;
    background: 0 0;
    padding: 0 .5rem;
    margin: 0;
    height: 1.35rem;
    white-space: nowrap;
    position: relative;
    text-overflow: ellipsis;
    font-size: .7rem;
    font-family: inherit;
    cursor: pointer;
}
.classification .ciic-list{
	height:2.2rem;
	padding:.5rem .4rem;
	float:left;
	text-align: center;
	font-size:.7rem;
}
.classification .button{
	color: #5f646e;
    font-size: .8rem;
    width: 100%;
    height: 2rem;
    line-height: 2rem;
    -webkit-box-flex: 1;
    -ms-flex: 1;
    border: 0;
    border-bottom: 2px solid transparent;
    border-radius: 0;
}
.classification .ciic-list a{
	display:block;
	width:100%;
	line-height:1.2rem;
	color:#666;
	position:relative;
}
.classification .ciic-list a.active:after{
	content:'';
    position:absolute;
    left:0;
    bottom:0;
    right:auto;
    top:auto;
    height:3px;
    width:100%;
    background-color:#d71416;
    display:block;
    z-index:2;
    -webkit-transform-origin:50% 100%;
    transform-origin:50% 100%;
    -webkit-transform: scaleY(.5);
    transform: scaleY(.5);
    backface-visibility:hidden;
}
#swiperindex{
	width:100%;
	padding:0;
}
#swiperindex .swiper-wrapper,#swiperindex .swiper-slide img{
	width:100%;
	height:100%;
}
#swiperindex .swiper-slide{
	position:relative;
	float: left;
    width: 100%;
    padding: 57% 0 0 0;
    text-align: center;
}
#swiperindex .swiper-slide .cover{
	position:absolute;
	display:block;
	top:0;
	left:0;
	bottom:0;
	right:0;
}
.swiper-container-horizontal>.swiper-pagination{
	position:absolute;
	bottom:0;
}
#swiperindex .swiper-pagination-bullet.swiper-pagination-bullet-active{
	background-color:#d71416
}
#swiperindex .swiper-pagination-bullet{
	background-color:#fff;
	opacity:.9;
}

