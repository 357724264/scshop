<#list list as product>
<a href="product!details.action?id=${product.id}">
	<div class="shops-img">
		<img src="${product.cover}"> <!-- 315*250 -->
	</div>
	<p class="spgj">
		<img src="${product.countryIcon}"><span>${product.country}</span>
	</p>
	<h3 class="shops-name">${product.name}</h3>
	<p class="shops-price">&yen;${product.bottomPrice}</p>
	<p class="shops-cdj">国内参考价&nbsp;&yen;&nbsp;${product.indicativePrice}</p>
</a>

</#list>