@CHARSET "UTF-8";
body,div,ul,li,dl,dt,dd,form,input,h1,h2,h3,h4,label,button,textarea,img{ 
	padding:0; margin:0; color:#222; font-size:14px; font-family:"Helvetica Neue","Hiragino Sans GB","Microsoft YaHei","\9ED1\4F53",Arial,sans-serif;
}
*{margin:0;padding:0;list-style:none;outline:none;}
button{cursor:pointer;border:0;}
a{text-decoration:none;}
a:hover{_zoom:1;}
img{border:none;}
.clear{clear:both;}
.fl{float:left;}
.fr{float:right;}
body{background: #e7e7ea;}
html,body{min-width:1024px;}
.noscroll{overflow-y: hidden;}
