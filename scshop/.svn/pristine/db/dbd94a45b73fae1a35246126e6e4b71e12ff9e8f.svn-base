	display: inline-block;
	height: 20px;
	width: 20px;	
	cursor: pointer;	
}
.zxmenu_tit .menu_do{
	position: absolute;	
	right: 12px;
}
.zxmenu_bar{
	position: relative;		
	border-bottom: 1px solid #ddd;
	z-index: 0;	
}
