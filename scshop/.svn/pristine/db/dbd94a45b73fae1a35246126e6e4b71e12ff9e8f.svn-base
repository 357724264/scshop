#uploader-demo{width:722px; height:100%; min-height:150px; padding:10px 0px; border:1px solid #c1c1c1;}
#fileList .file-item{list-style:none; float:left; list-style:none; float:left; width:150px; height:150px; position:relative; margin:0px 0px 8px 8px;}
.fileInput{position:absolute; top:0px; left:0px; width:150px; height:150px; font-size:160px; opacity: 0;
    filter:alpha(opacity=0);
    cursor:pointer;}
.delete{padding:0px 10px 15px 20px; box-sizing:border-box; font-weight:600; background:rgba(255,255,255,0.8); display:block; position:absolute; top:0px; right:0px;  border-bottom-left-radius:40px; text-align:center; color:red; font-size:20px;}
#filePicker{
    width:150px; 
    height:150px; 
    line-height:150px; 
    font-size:160px; 
    color:#c1c1c1; 
    text-align:center; 
    border:1px dotted #c1c1c1; 
    display:inline-block; 
    cursor:pointer;
    box-sizing:border-box;
    margin-left:8px;
    position:relative;
}
.addfilePicker{
    width:150px; 
    height:150px; 
    line-height:150px; 
    font-size:120px; 
    color:#c1c1c1; 
    text-align:center; 
    
}
.webuploader-element-invisible{
    height:150px;
    width:150px; 
    position:absolute;
    top:0px;
    left:0px;
    overflow: hidden;
    font-size: 300px;
    opacity: 0;
    filter:alpha(opacity=0);
    cursor:pointer;
}
.file{ 
    height:100%;
    width:100%; 
    position:absolute;
    top:0px;
    left:0px;
    overflow: hidden;
    font-size: 300px;
    opacity: 0;
    filter:alpha(opacity=0);
    cursor:pointer;
}



.add-box{width:100%; height:auto;}
.add-box ul li{list-style:none; display:inline-block; position:relative; float:left; width:55px; text-align:center; height:26px; line-height:26px; font-size:14px; color:#666; border:1px solid #c1c1c1; border-radius:2px; margin-left:5px; overflow:hidden; white-space:nowrap; text-overflow:ellipsis;}
.add-box ul li .close{position:absolute; top:0px; right:0px; color:#fff; width:12px; height:12px; line-height:12px; background:rgba(204,0,51,0.5); cursor:pointer;}
.add-box #add-btn{width:50px; height:28px; margin-left:5px; line-height:28px; position:relative; overflow:hidden; border:1px solid #c1c1c1; text-align:center; display:inline-block; text-decoration:none; color:#666; outline:none; cursor:pointer; box-sizing:border-box;}

.add-box .text{width:100%; height:100%; box-sizing:border-box; outline:none; border:0px; text-align:center;}

