}

/* 表单提交加载等待 */
#loading{
	display: none;
	position: fixed;
	height: 100%;
	width: 100%;
	left: 0;
	top: 0;
	z-index: 7000;
}
#loadtext{
	width: 254px;
	height: 94px;
	padding: 8px 0;
	position: fixed;
	top: 50%;
	left: 50%;
	margin-left: -130px;
	margin-top: -55px;
	text-align: center;
	font-size: 15px;
	z-index: 7001;
	border-radius: 5px;
}
#loadtext p{
	margin-top: 5px;
	color: #fff;
}
#loadtext img{
	width: 60px;
	height: 60px;
}


