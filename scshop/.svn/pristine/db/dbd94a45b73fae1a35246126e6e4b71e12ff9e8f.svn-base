	background: #36a65a;
	cursor: pointer;	
}
.zxmenu_bar.on span{
	color: #fff;	
}
.zxmenu_link:hover strong,.zxmenu_link.on strong{
	display: block;	
}
.zxmenu_link{
	padding: 10px 12px 10px 0;	
	height: 20px;
	line-height: 20px;	
	display: block;
} 
.zxmenu_bar span{
	font-size: 14px;
	color: #333;
	width: 125px;
	padding: 10px 0;
	margin: -10px 0;
	display: inline-block;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;	
}
.zxmenu_bar strong{
	display: none;
	position: absolute;	
	right: 12px;
	top: 10px;
	z-index: 11;
}
.zxmenu_bar strong b{
	margin-left: 8px;
}
